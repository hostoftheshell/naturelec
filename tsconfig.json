{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"strictNullChecks": true,
		"allowJs": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react-jsx",
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"],
			"@images/*": ["src/assets/images/*"],
			"@videos/*": ["src/assets/videos/*"]
		},
		"jsxImportSource": "react"
	}
}
